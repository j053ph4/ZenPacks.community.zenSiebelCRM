<object id='/zport/dmd/Events/App/Siebel' module='Products.ZenEvents.EventClass' class='EventClass'>
<property type="text" id="transform" mode="w" >
searchString = "Run State"
if evt.summary.find(searchString) &gt;= 0:
	value = evt.summary[evt.summary.find("current value ")+14:len(evt.summary)-1]
	status = "BLAH"
	val = int(float(value))
	if val == -1:
		evt.severity = 2
		status = "Unknown"
	if val == 0:
		evt.severity = 3
		status = "Shutdown"
	if val == 1:
		evt.severity = 3
		status = "Unavailable"
	if val == 2:
		evt.severity = 4
		status = "Stopped"
	if val == 3:
		evt.severity = 0
		status = "Online"
	if val == 4:
		evt.severity = 0
		status = "Running"
	evt.summary = evt.component+" is "+status
</property>
</object>
